CC=arm-linux-gnueabihf-gcc
CXX=arm-linux-gnueabihf-g++
CPP=arm-linux-gnueabihf-gcc -E
LD=arm-linux-gnueabihf-ld.gold
OS=linux
CFLAGS=-O3 -g -pipe -march=armv7-a -mtune=cortex-a9 -mcpu=cortex-a9 -mfloat-abi=hard -mfpu=neon -mabi=aapcs-linux -Wno-psabi -Wa,-mno-warn-deprecated -Wno-deprecated-declarations
CPPFLAGS=$CFLAGS
CXXFLAGS=$CFLAGS
